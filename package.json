{
  "name": "react-mention",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-mentions": "^3.3.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "node-sass src/sass/main.scss src/App.css -w",
    "start:sass": "npm-run-all --parallel watch:sass",
    "compile:sass": "node-sass src/sass/main.scss src/App.comp.css",
    "concat:css": "concat -o src/App.concat.css src/App.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/App.concat.css -o src/App.prefix.css",
    "compress:css": "node-sass src/App.prefix.css src/App.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "^1.0.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
